<?xml version="1.0" encoding="us-ascii" ?>
<xsl:transform
 version="3.0"
 xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
 >
 <xsl:output indent="no" />
 <xsl:variable name="rdfns" >
  http://www.w3.org/1999/02/22-rdf-syntax-ns#
 </xsl:variable>
 <xsl:variable name="rdf" select="normalize-space($rdfns)" />
 <xsl:variable name="n" >1</xsl:variable>
 <xsl:template name="initial-template" match="/" >
   <xsl:message>init</xsl:message>
   <xsl:apply-templates select="unparsed-text-lines('i.txt')" />
 </xsl:template>
 <xsl:template match=".[starts-with(.,'j')]" >
   <xsl:value-of select="." ></xsl:value-of>
 </xsl:template>
 <xsl:template name="demo" >
   <xsl:message>demo</xsl:message>
  <xsl:text disable-output-escaping="yes" expand-text="yes">
   &lt;!DOCTYPE rdf:RDF [
   &lt;!ENTITY rdf '{$rdf}'&gt;
   ]&gt;
  </xsl:text>
  <xsl:text disable-output-escaping="yes" >
   &lt;RDF xmlns="&amp;rdf;"&gt;
  </xsl:text>
  <xsl:text disable-output-escaping="yes" expand-text="yes" >
   &lt;Description about="&amp;rdf;_{$n}"&gt;
  </xsl:text>
  <xsl:element name="_{$n}" >153715</xsl:element>
  <xsl:text disable-output-escaping="yes" >
   &lt;/Description&gt;
  </xsl:text>
  <xsl:text disable-output-escaping="yes" >
   &lt;/RDF&gt;
  </xsl:text>
 </xsl:template>
</xsl:transform>
